<?xml version="1.0" encoding="UTF-8"?>
<tmx version="2.0" xmlns="http://www.lisa.org/tmx20">
    <header adminlang="en" creationtool="locale4j"
        creationtoolversion="1.1" o-tmf="unknown" segtype="block" srclang="*all*" />
    <body>

        <tu tuid="pageTitle">
            <tuv xml:lang="fr">
                <seg>Réinitialisation de votre mot de passe</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Reset of your password</seg>
            </tuv>
        </tu>

        <!-- Formulaire de demande de réinitialisation du mot de passe -->

        <tu tuid="captchaFormDescriptionSentence">
            <tuv xml:lang="fr">
                <seg>Remplissez ce premier formulaire afin de recevoir un code de vérification par e-mail.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Please fill in this first form in order to receive the verification code.</seg>
            </tuv>
        </tu>
        <tu tuid="captchaFieldLabel">
            <tuv xml:lang="fr">
                <seg>Merci de saisir ci-dessous les caractères figurant dans l'image ci-contre</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Please enter below the types in the picture opposite</seg>
            </tuv>
        </tu>

        <!-- Validation du formulaire -->

        <tu tuid="invalidCaptchaInput">
            <tuv xml:lang="fr">
                <seg>Les caractères saisis ne correspondent pas à ceux de l'image.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Entered types do not match those of the image above.</seg>
            </tuv>
        </tu>

        <!-- Messages -->

        <tu tuid="emailNewPasswordLinkSent">
            <tuv xml:lang="fr">
                <seg>Le code de vérification a été envoyé à l'adresse e-mail indiquée.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Verification code was sent to the typed in e-mail address.</seg>
            </tuv>
        </tu>

        <!-- Formulaire de saisie du code de vérification -->

        <tu tuid="confirmationFormDescriptionSentence">
            <tuv xml:lang="fr">
                <seg>Entrez le code de confirmation reçu par email.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Please type in the confirmation code received by e-mail.</seg>
            </tuv>
        </tu>
        <tu tuid="confirmationCodeLabel">
            <tuv xml:lang="fr">
                <seg>Code de confirmation</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Confirmation code</seg>
            </tuv>
        </tu>

        <tu tuid="confirmationCodeInvalid">
            <tuv xml:lang="fr">
                <seg>Code de confirmation invalide.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Invalid confirmation code.</seg>
            </tuv>
        </tu>

    </body>
</tmx>
