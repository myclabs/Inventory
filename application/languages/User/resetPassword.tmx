<?xml version="1.0" encoding="UTF-8"?>
<tmx version="2.0" xmlns="http://www.lisa.org/tmx20">
    <header adminlang="en" creationtool="locale4j"
        creationtoolversion="1.1" o-tmf="unknown" segtype="block" srclang="*all*" />
    <body>

        <!-- Formulaire de réinitialisation du mot de passe -->

        <tu tuid="pageTitle">
            <tuv xml:lang="fr">
                <seg>Réinitialisation de votre mot de passe</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Reset of your password</seg>
            </tuv>
        </tu>
        <tu tuid="captchaFieldLabel">
            <tuv xml:lang="fr">
                <seg>Merci de saisir ci-dessous les caractères figurant dans l'image ci-contre</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Please enter below the types in the picture opposite</seg>
            </tuv>
        </tu>

        <!-- Validation du formulaire -->

        <tu tuid="invalidCaptchaInput">
            <tuv xml:lang="fr">
                <seg>Les caractères saisis ne correspondent pas à ceux de l'image.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Entered types do not match those of the image above.</seg>
            </tuv>
        </tu>

    </body>
</tmx>