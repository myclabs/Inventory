<?xml version="1.0" encoding="UTF-8"?>
<tmx version="2.0" xmlns="http://www.lisa.org/tmx20">
    <header adminlang="en" creationtool="locale4j"
        creationtoolversion="1.1" o-tmf="unknown" segtype="block" srclang="*all*" />
    <body>
    
        <!-- Datagrid -->

        <tu tuid="directPredicateLabelHeader">
            <tuv xml:lang="fr">
                <seg>Libellé prédicat direct</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Direct predicate label</seg>
            </tuv>
        </tu>
        <tu tuid="directPredicateIdentifierHeader">
            <tuv xml:lang="fr">
                <seg>Identifiant prédicat direct</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Direct predicate identifier</seg>
            </tuv>
        </tu>
        <tu tuid="reversePredicateLabelHeader">
            <tuv xml:lang="fr">
                <seg>Libellé prédicat inverse</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Reverse predicate label</seg>
            </tuv>
        </tu>
        <tu tuid="reversePredicateIdentifierHeader">
            <tuv xml:lang="fr">
                <seg>Identifiant prédicat inverse</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Reverse predicate identifier</seg>
            </tuv>
        </tu>
        <tu tuid="addPanelTitle">
            <tuv xml:lang="fr">
                <seg>Ajout d'une paire prédicat / prédicat inverse</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Addition of a predicate/inverse predicate pair</seg>
            </tuv>
        </tu>

        <!-- Messages -->

        <tu tuid="refIsSameAsRevRef">
            <tuv xml:lang="fr">
                <seg>Merci de saisir des identifiants différents pour les prédicats direct et inverse.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Please do not choose the same identifier for predicate and inverse predicate.</seg>
            </tuv>
        </tu>
        <tu tuid="predicateUsedInAssociation">
            <tuv xml:lang="fr">
                <seg>Ce prédicat ne peut pas être supprimé, car il est utilisé dans (au moins) une relation entre mots clés.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>This predicate cannot be deleted, since it is involved in (at least) one relation between keywords.</seg>
            </tuv>
        </tu>


    </body>
</tmx>