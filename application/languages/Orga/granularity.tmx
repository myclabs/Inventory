<?xml version="1.0" encoding="UTF-8"?>
<tmx version="2.0" xmlns="http://www.lisa.org/tmx20">
	<header adminlang="en" creationtool="locale4j"
		creationtoolversion="1.1" o-tmf="unknown" segtype="block" srclang="*all*" />
	<body>

        <tu tuid="granularity">
            <tuv xml:lang="fr">
                <seg>Niveau</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Level</seg>
            </tuv>
        </tu>
        <tu tuid="granularities">
            <tuv xml:lang="fr">
                <seg>Niveaux</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Levels</seg>
            </tuv>
        </tu>

        <!-- Datagrid -->

        <!-- Colonne "Navigable ?" -->

		<tu tuid="navigableHeader">
			<tuv xml:lang="fr">
				<seg>Navigable ?</seg>
			</tuv>
			<tuv xml:lang="en">
				<seg>Navigable ?</seg>
			</tuv>
		</tu>
		<tu tuid="navigableOption">
			<tuv xml:lang="fr">
				<seg>Navigable</seg>
			</tuv>
			<tuv xml:lang="en">
				<seg>Navigable</seg>
			</tuv>
		</tu>
		<tu tuid="notNavigableOption">
			<tuv xml:lang="fr">
				<seg>Non navigable</seg>
			</tuv>
			<tuv xml:lang="en">
				<seg>Not navigable</seg>
			</tuv>
		</tu>


        <tu tuid="labelGlobalGranularity">
			<tuv xml:lang="fr">
				<seg>Niveau organisationnel global</seg>
			</tuv>
			<tuv xml:lang="en">
				<seg>Global organizational level</seg>
			</tuv>
		</tu>
		<tu tuid="addPanelTitle">
			<tuv xml:lang="fr">
				<seg>Ajout d'un niveau organisationnel</seg>
			</tuv>
			<tuv xml:lang="en">
				<seg>Addition of an organizational level</seg>
			</tuv>
		</tu>

        <!-- Messages -->

        <!-- Ajout -->

        <tu tuid="addGranularitySelectAxes">
            <tuv xml:lang="fr">
                <seg>Axes du niveau à ajouter</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Axes of the level to be created</seg>
            </tuv>
        </tu>
        <tu tuid="addGranularitySelectAxesInputLevel">
            <tuv xml:lang="fr">
                <seg>Niveau de saisie</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Input level</seg>
            </tuv>
        </tu>
        <tu tuid="addGranularityFormChoiceSelectAxes">
            <tuv xml:lang="fr">
                <seg>Niveau de choix des formulaires</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Level for the choice of forms</seg>
            </tuv>
        </tu>
        <tu tuid="addGranularityInputSelectAxes">
            <tuv xml:lang="fr">
                <seg>Niveau pour la saisie</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Level for input</seg>
            </tuv>
        </tu>
        <tu tuid="emptyGranularity">
            <tuv xml:lang="fr">
                <seg>Merci de sélectionner au moins un axe.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Please select at least one axis.</seg>
            </tuv>
        </tu>
        <tu tuid="granularityAlreadyExists">
            <tuv xml:lang="fr">
                <seg>Il existe déjà un niveau organisationnel correspondant à cette combinaison d'axes.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>An organizational level with the same axes already exists.</seg>
            </tuv>
        </tu>
        <tu tuid="hierarchicallyLinkedAxes">
            <tuv xml:lang="fr">
                <seg>Merci de sélectionner des axes deux à deux non hiérarchiquement reliés.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Please select a set of axes without any pair of hierarchically linked axes.</seg>
            </tuv>
        </tu>

        <!-- Modification -->

        <tu tuid="roleExistsForCellAtThisGranularity">
            <tuv xml:lang="fr">
                <seg>Cette modification ne peut pas être effectuée, car il existe au moins un rôle associé à une unité organisationnelle de ce niveau organisationnel.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>This change cannot be performed, since there exists at least one role for an organizational unit at this organizational level.</seg>
            </tuv>
        </tu>

        <!-- Suppression -->

        <tu tuid="granularityCantBeDeleted">
            <tuv xml:lang="fr">
                <seg>Ce niveau organisationnel ne peut pas être supprimé, car il est utilisé pour la configuration
     générale de l'organisation (statut des collectes, rôles, sélection des formulaires comptables, saisies).</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>This organizational level cannot be deleted, since it is used for the general configuration
                of this organization (collection data status, roles, accounting form selection, inputs).</seg>
            </tuv>
        </tu>
        <tu tuid="deleteGranularityInventory">
            <tuv xml:lang="fr">
                <seg>Ce niveau organisationnel ne peut pas être supprimé, car il a été choisi comme niveau de
                    définition du statut des collectes.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>This organizational level cannot be deleted, since it has been chosen as the level
                    where collection data status are defined.</seg>
            </tuv>
        </tu>
        <tu tuid="deleteGranularityConfigAF">
            <tuv xml:lang="fr">
                <seg>Ce niveau organisationnel ne peut pas être supprimé, car il a été sélectionné pour y effectuer
                    le choix de formulaires comptables (voir l'onglet « Informations générales »).</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>This organizational level cannot be deleted, since it has been selected for the choice of
                    accounting forms (see "General info" tab).</seg>
            </tuv>
        </tu>
        <tu tuid="deleteGranularityInputAF">
            <tuv xml:lang="fr">
                <seg>Ce niveau organisationnel ne peut pas être supprimé, car il a été sélectionné pour y effectuer
                    des saisies (voir l'onglet « Informations générales »).</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>This organizational level cannot be deleted, since it has been selected for input processing (see
                    "General info" tab).</seg>
            </tuv>
        </tu>

	</body>
</tmx>