{
    "autoload": {
        "psr-0": {
            "": "src/"
        },
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "src/Core/Translation/functions.php"
        ]
    },
    "require": {
        "zendframework/zendframework1": "1.12.*",
        "doctrine/orm": "2.5.*@dev",
        "doctrine/annotations": "~1.0",
        "gedmo/doctrine-extensions": "dev-OptimizedTranslatable as 2.99",
        "symfony/event-dispatcher": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/translation": "~2.3",
        "symfony/yaml": "~2.3",
        "pear/console_commandline": "dev-trunk",
        "netcarver/textile": "~3.5",
        "dompdf/dompdf": "dev-master",
        "phpoffice/phpexcel": "dev-WriterExcel5OptionalConstructor as 1.7.99",
        "myclabs/array-comparator": "1.0.*",
        "myclabs/xport": "0.2.*",
        "myclabs/php-enum": "1.*",
        "myclabs/deep-copy": "1.*",
        "myclabs/acl": "dev-simplified-model",
        "mnapoli/php-di": "~4.0",
        "mnapoli/php-di-zf1": "*",
        "psr/log": "~1.0",
        "monolog/monolog": "~1.6",
        "mnapoli/number-two": "~0.2.0",
        "myclabs/work": "~0.2.0",
        "videlalvaro/php-amqplib": "~2.2"
    },
    "require-dev": {
        "behat/behat": "~2.4",
        "behat/mink": "~1.5",
        "behat/mink-extension": "*",
        "behat/mink-selenium2-driver": "*",
        "mnapoli/metaconsole": "dev-master"
    },
    "suggest": {
        "shvetsgroup/parallelrunner": "Run Behat scenarios in parallel"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        {
            "type": "git",
            "url": "git://github.com/myclabs/PHPExcel.git"
        },
        {
            "type": "git",
            "url": "git://github.com/myclabs/DoctrineExtensions.git"
        },
        {
            "type": "git",
            "url": "git@github.com:myclabs/ACL.git"
        }
    ]
}
